{
  "name": "deepagents",
  "version": "1.2.0",
  "description": "Deep Agents - a library for building controllable AI agents with LangGraph",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsdown",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts examples/**/*.ts",
    "lint:fix": "eslint src/**/*.ts examples/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\" \"examples/**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json,md}\" \"examples/**/*.{ts,js,json,md}\"",
    "prepublishOnly": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "start": "node dist/index.js",
    "release": "pnpm build && changeset publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/langchain-ai/deepagentsjs.git"
  },
  "keywords": [
    "ai",
    "agents",
    "langgraph",
    "langchain",
    "typescript",
    "llm"
  ],
  "author": "LangChain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/langchain-ai/deepagentsjs/issues"
  },
  "homepage": "https://github.com/langchain-ai/deepagentsjs#readme",
  "dependencies": {
    "@ag-ui/client": "^0.0.41",
    "@anthropic-ai/sdk": "^0.71.0",
    "@copilotkit/runtime": "^1.10.6",
    "@copilotkit/sdk-js": "^1.10.6",
    "@langchain/anthropic": "^1.0.0",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^1.1.4",
    "@langchain/langgraph": "^1.0.4",
    "@langchain/langgraph-checkpoint-postgres": "^1.0.0",
    "@langchain/openai": "^0.3.0",
    "@langchain/textsplitters": "^0.1.0",
    "@supabase/supabase-js": "^2.58.0",
    "cors": "^2.8.5",
    "deepagents": "^1.2.0",
    "express": "^5.1.0",
    "fast-glob": "^3.3.3",
    "langchain": "^1.0.4",
    "mammoth": "^1.8.0",
    "micromatch": "^4.0.8",
    "multer": "^1.4.5-lts.1",
    "openai": "^6.9.1",
    "pdf-parse": "^1.1.1",
    "replicate": "^1.4.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.19.0",
    "@langchain/langgraph-checkpoint": "^1.0.0",
    "@langchain/tavily": "^1.0.1",
    "@tsconfig/recommended": "^1.0.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.5",
    "@types/micromatch": "^4.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.5",
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^2.1.8",
    "dotenv": "^17.2.3",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "globals": "^15.0.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.12",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.22.0",
    "uuid": "^11.0.5",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist/**/*"
  ]
}
